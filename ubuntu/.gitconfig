[user]
	name = "Alexander Kharchenko"
	email = morontt@yandex.ru
	signingkey = BCE79440
[core]
	excludesfile = ~/.gitignore_global
	autocrlf = input
	editor = nano
[color]
	ui = true
[alias]
	co = checkout
	br = branch
	ci = commit
	fck = commit --amend --no-edit
	rb = rebase
	cp = cherry-pick
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[push]
	default = current
[merge]
	conflictstyle = diff3
[pull]
	rebase = false
